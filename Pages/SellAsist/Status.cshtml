@page
@model Sellasist_Optima.Pages.StatusModel

<h2>Statusy</h2>

@if (Model.Status != null && Model.Status.Count > 0)
{
    <ul>
        @foreach (var status in Model.Status)
        {
            <li>@status.Name</li>
        }
    </ul>
}
else
{
    <p>Brak logowania do SellAsist</p>
    <p>Przejdź na zakładkę API</p>
}


